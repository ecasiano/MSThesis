@article{1751-8121-42-50-504012,
	Abstract = {We review 'particle-partitioning entanglement' for itinerant many-particle systems. This is defined as the entanglement between two subsets of particles making up the system. We identify generic features and mechanisms of particle entanglement that are valid over whole classes of itinerant quantum systems. We formulate the general structure of particle entanglement in many-fermion ground states, analogous to the 'area law' for the more usually studied entanglement between spatial regions. The basic properties of particle entanglement are first elucidated by considering relatively simple itinerant models. We then review particle-partitioning entanglement in quantum states with more intricate physics, such as anyonic models and quantum Hall states.},
	Author = {Masudul Haque and O S Zozulya and K Schoutens},
	Date-Added = {2018-03-16 19:06:49 +0000},
	Date-Modified = {2018-03-16 19:06:49 +0000},
	Journal = {Journal of Physics A: Mathematical and Theoretical},
	Number = {50},
	Pages = {504012},
