@article{1742-5468-2012-08-P08011,
	Abstract = {We study the entanglement spectrum (ES) of a finite XXZ spin- ##IMG## [http://ej.iop.org/images/1742-5468/2012/08/P08011/jstat440419ieqn85.gif] {$\frac{1}{2}$} chain in the limit Î â â1 + for both open and periodic boundary conditions. At Î =â 1 (ferromagnetic point) the model is equivalent to the Heisenberg ferromagnet and its degenerate ground state manifold is the S U (2) multiplet with maximal total spin. Any state in this so-called `symmetric sector' is an equal weight superposition of all possible spin configurations. In the gapless phase at Î >â 1 this property is progressively lost as one moves away from the Î =â 1 point. We investigate how the ES obtained from the states in this manifold reflects this change, using exact diagonalization and Bethe ansatz calculations. We find that in the limit Î â â1 + most of the ES levels show divergent behavior. Moreover, while at Î =â 1 the ES contains no information about the boundaries, for Î >â 1 it depends dramatically on the choice of boundary conditions. For both open and periodic boundary conditions the ES exhibits an elegant multiplicity structure for which we conjecture a combinatorial formula. We also study the entanglement eigenfunctions, i.e. the eigenfunctions of the reduced density matrix. We find that the eigenfunctions corresponding to the non-diverging levels mimic the behavior of the state wavefunction, whereas the others show intriguing polynomial structures. Finally we analyze the distribution of the ES levels as the system is detuned away from Î =â 1.},
	Author = {Vincenzo Alba and Masudul Haque and Andreas M L{\"a}uchli},
	Date-Added = {2019-02-19 10:41:51 -0500},
	Date-Modified = {2019-02-19 10:41:51 -0500},
	Journal = {Journal of Statistical Mechanics: Theory and Experiment},
	Number = {08},
	Pages = {P08011},
	Title = {Entanglement spectrum of the Heisenberg XXZ chain near the ferromagnetic point},
	Url = {http://stacks.iop.org/1742-5468/2012/i=08/a=P08011},
	Volume = {2012},
	Year = {2012},
	Bdsk-Url-1 = {http://stacks.iop.org/1742-5468/2012/i=08/a=P08011}}

